<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="nyc.c4q.capstone.feed.MainFeedFragment"
    >

  <!-- TODO: Update blank fragment layout -->
  <TextView
      android:id="@+id/firebase_test_title"
      android:layout_width="200dp"
      android:layout_height="50dp"
      android:layout_marginTop="120dp"
      android:text="@string/hello_blank_fragment"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      />
  <TextView
      android:id="@+id/firebase_test_goal"
      android:layout_width="200dp"
      android:layout_height="50dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="32dp"
      android:text="@string/hello_blank_fragment"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@id/firebase_test_title"
      />
  <TextView
      android:id="@+id/firebase_test_imageurl"
      android:layout_width="200dp"
      android:layout_height="50dp"
      android:layout_marginEnd="8dp"
      android:layout_marginStart="8dp"
      android:layout_marginTop="32dp"
      android:text="@string/hello_blank_fragment"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@id/firebase_test_goal"
      />

</android.support.constraint.ConstraintLayout>
